using System;
					
public class Program
{
	public static void Main()
	{   Console.WriteLine("ingrese y , z:");
	
		double y = double.Parse(Console.ReadLine());
	    double z = double.Parse(Console.ReadLine());
	 double t = Math.Sqrt( (z*z) + (y*y));
	  double w = (z/t); 
		double c = Math.Asin(w) * (180/Math.PI);
	  double a = 180 -90 - c;
	    Console.WriteLine(t);
		Console.WriteLine(c);
	 Console.WriteLine(a);
			
		
	}
}
